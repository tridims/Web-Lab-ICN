// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  ID Int @id @default(autoincrement())
  Name String?
  Email String? @unique
  Password String?
}

model Model{
  ID Int @id @default(autoincrement())
  Name String?
  Description String?
}